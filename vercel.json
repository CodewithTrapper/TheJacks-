{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/Styles/(.*)",
      "dest": "/Styles/$1"
    },
    {
      "src": "/vid/(.*)",
      "dest": "/vid/$1"
    },
    {
      "src": "/Images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/icons/(.*)",
      "dest": "/icons/$1"
    },
    {
      "src": "/(.*\\.(html|css|js|png|jpg|jpeg|gif|svg|ico))",
      "dest": "/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/mpesa/callback",
      "dest": "/server.js"
    },
    {
      "src": "/(login|guestlogin|adminlogin|logout|adminlogout|add-guest|add-room|subscribe|admin|loginAdmin|loginGuest|book-room|book-table|forgotpassword|resetpassword/.*|pay/mpesa|admin/sendNewsletter)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}