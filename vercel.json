{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/(roombooking|adminstatus|newsletter|payment|profit|manageRooms|manageAdmins|homePage|bookings|adminlogin|guestregistration|guestlogin|passwordreset|reservation|book-table|activitystatus|frequentlyaskedquestions|notifications|admin-simple|login|logout|adminlogout|add-guest|add-room|subscribe|admin|loginAdmin|loginGuest|book-room|forgotpassword|resetpassword/.*|pay/mpesa|admin/sendNewsletter|api/.*|mpesa/.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
